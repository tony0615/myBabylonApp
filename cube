        <table>
            <tr><td>上：<input type="text" class="chrinput" id="plat4" value="上" /></td><td>下：<input type="text" class="chrinput" id="plat5" value="下" /></td></tr>
            <tr><td>左：<input type="text" class="chrinput" id="plat3" value="左" /></td><td>右：<input type="text" class="chrinput" id="plat2" value="右" /></td></tr>
            <tr><td>前：<input type="text" class="chrinput" id="plat1" value="前" /></td><td>后：<input type="text" class="chrinput" id="plat0" value="后" /></td></tr>
            <tr><td colspan="2"><button onclick="changechr()">变换</button><button onclick="changemodel('231')">231模型</button><button onclick="changemodel('222')">222模型</button><button onclick="changemodel('141')">141模型</button></td></tr>
            <tr><td colspan="2"><button onclick="changemodel('33')">33模型</button><button onclick="changemodel('141')">141模型</button><button onclick="changemodel('1113')">1113模型</button></td></tr>
